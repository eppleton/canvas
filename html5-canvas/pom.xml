        <dependency>
            <groupId>org.openide</groupId>
            <artifactId>org.openide.util.lookup</artifactId>
            <version>7.4-SNAPSHOT</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.netbeans.html</groupId>
